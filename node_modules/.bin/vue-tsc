#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wouter/Documents/Wisemen/Libraries/wisemen-vue-core/node_modules/.pnpm/vue-tsc@2.0.6_typescript@5.3.3/node_modules/vue-tsc/bin/node_modules:/Users/wouter/Documents/Wisemen/Libraries/wisemen-vue-core/node_modules/.pnpm/vue-tsc@2.0.6_typescript@5.3.3/node_modules/vue-tsc/node_modules:/Users/wouter/Documents/Wisemen/Libraries/wisemen-vue-core/node_modules/.pnpm/vue-tsc@2.0.6_typescript@5.3.3/node_modules:/Users/wouter/Documents/Wisemen/Libraries/wisemen-vue-core/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/wouter/Documents/Wisemen/Libraries/wisemen-vue-core/node_modules/.pnpm/vue-tsc@2.0.6_typescript@5.3.3/node_modules/vue-tsc/bin/node_modules:/Users/wouter/Documents/Wisemen/Libraries/wisemen-vue-core/node_modules/.pnpm/vue-tsc@2.0.6_typescript@5.3.3/node_modules/vue-tsc/node_modules:/Users/wouter/Documents/Wisemen/Libraries/wisemen-vue-core/node_modules/.pnpm/vue-tsc@2.0.6_typescript@5.3.3/node_modules:/Users/wouter/Documents/Wisemen/Libraries/wisemen-vue-core/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vue-tsc@2.0.6_typescript@5.3.3/node_modules/vue-tsc/bin/vue-tsc.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vue-tsc@2.0.6_typescript@5.3.3/node_modules/vue-tsc/bin/vue-tsc.js" "$@"
fi
