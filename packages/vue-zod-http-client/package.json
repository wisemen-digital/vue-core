{
  "name": "@wisemen/zod-http-client",
  "type": "module",
  "version": "0.0.4",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/node": "^20.6.3",
    "@wisemen/eslint-config-vue": "0.2.0",
    "bumpp": "^9.2.0",
    "eslint": "9.10.0",
    "esno": "^0.17.0",
    "pnpm": "^8.7.6",
    "rimraf": "^5.0.1",
    "typescript": "5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.5"
  }
}
