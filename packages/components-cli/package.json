{
  "name": "wisemen-ui",
  "type": "module",
  "version": "0.2.2",
  "description": "Add components to your apps.",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Robbe Vaes"
  },
  "license": "MIT",
  "keywords": [
    "components",
    "ui",
    "tailwind",
    "vue",
    "shadcn"
  ],
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist && rimraf components",
    "start:dev": "cross-env COMPONENTS_BASE_URL=http://localhost:3001 node dist/index.js",
    "start": "node dist/index.js",
    "registry": "tsx ./src/generate/generateRegistry.ts",
    "changeset": "changeset",
    "release": "changeset version",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",

    "pub:beta": "pnpm lint && pnpm typecheck && pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm build && ppnpm lint && pnpm typecheck && npm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm typecheck && pnpm build && pnpm publish --access public"
  },
  "dependencies": {
    "chalk": "5.2.0",
    "commander": "^10.0.0",
    "cosmiconfig": "^8.1.3",
    "diff": "^5.1.0",
    "execa": "^7.0.0",
    "find-up": "^6.3.0",
    "fs-extra": "^11.1.0",
    "https-proxy-agent": "^6.2.0",
    "node-fetch": "^3.3.0",
    "ora": "^6.1.2",
    "prompts": "^2.4.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/diff": "^5.0.3",
    "@types/fs-extra": "^11.0.1",
    "@types/prompts": "^2.4.2",
    "changeset": "^0.2.6",
    "eslint": "8.57",
    "rimraf": "^4.1.3",
    "tsup": "^6.6.3",
    "type-fest": "^3.8.0",
    "typescript": "^4.9.3"
  }
}
