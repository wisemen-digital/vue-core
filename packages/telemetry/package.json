{
  "name": "@wisemen/vue-core-telemetry",
  "type": "module",
  "version": "0.0.3",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typings": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "cleanup": "rimraf dist/ node_modules/ .turbo/",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish",
    "type-check": "tsc --noEmit",
    "pub:beta": "pnpm publish --no-git-checks --access public --tag beta"
  },
  "peerDependencies": {
    "vue": "catalog:peer"
  },
  "dependencies": {
    "@opentelemetry/context-zone": "catalog:telemetry",
    "@opentelemetry/core": "catalog:telemetry",
    "@opentelemetry/exporter-trace-otlp-http": "catalog:telemetry",
    "@opentelemetry/instrumentation": "catalog:telemetry",
    "@opentelemetry/instrumentation-fetch": "catalog:telemetry",
    "@opentelemetry/instrumentation-user-interaction": "catalog:telemetry",
    "@opentelemetry/otlp-exporter-base": "catalog:telemetry",
    "@opentelemetry/resources": "catalog:telemetry",
    "@opentelemetry/sdk-trace-base": "catalog:telemetry",
    "@opentelemetry/sdk-trace-web": "catalog:telemetry",
    "@sentry/vue": "catalog:telemetry"
  },
  "devDependencies": {
    "@wisemen/eslint-config-vue": "catalog:lint",
    "bumpp": "catalog:build",
    "eslint": "catalog:lint",
    "rimraf": "catalog:build",
    "tsdown": "catalog:build",
    "typescript": "catalog:build"
  }
}
