{
  "name": "@wisemen/vue-core-telemetry",
  "type": "module",
  "version": "0.0.2",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "cleanup": "rimraf dist/ node_modules/ .turbo/",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "3.5.18"
  },
  "dependencies": {
    "@opentelemetry/context-zone": "2.0.1",
    "@opentelemetry/core": "2.0.1",
    "@opentelemetry/exporter-trace-otlp-http": "0.203.0",
    "@opentelemetry/instrumentation": "0.203.0",
    "@opentelemetry/instrumentation-fetch": "0.203.0",
    "@opentelemetry/instrumentation-user-interaction": "0.48.0",
    "@opentelemetry/otlp-exporter-base": "0.203.0",
    "@opentelemetry/resources": "2.0.1",
    "@opentelemetry/sdk-trace-base": "2.0.1",
    "@opentelemetry/sdk-trace-web": "2.0.1",
    "@opentelemetry/semantic-conventions": "1.36.0",
    "@sentry/integrations": "7.114.0",
    "@sentry/tracing": "7.120.4",
    "@sentry/vue": "10.0.0"
  },
  "devDependencies": {
    "@number-flow/vue": "0.4.8",
    "@types/node": "catalog:",
    "@wisemen/eslint-config-vue": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "esno": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vitest-localstorage-mock": "0.1.2"
  }
}
