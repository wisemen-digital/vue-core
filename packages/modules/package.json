{
  "name": "@wisemen/vue-core-modules",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "version": "3.0.0",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "prerelease": "pnpm version prerelease --preid=alpha",
    "build": "vite build",
    "precommit": "pnpm type-check && test:unit && pnpm build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc -p tsconfig.check.json --noEmit",
    "pub:release": "pnpm publish --access public",
    "test:unit": "vitest --run",
    "test": "vitest --run",
    "lint": "eslint .",
    "cleanup": "rimraf dist/ node_modules/ .turbo/",
    "lint:fix": "eslint . --fix",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@wisemen/vue-core-components": "workspace:*",
    "motion-v": "catalog:",
    "vue": "catalog:",
    "vue-i18n": "catalog:"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "1.0.0-alpha.2",
    "@tailwindcss/vite": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/test-utils": "catalog:",
    "@vue/tsconfig": "catalog:",
    "@vueuse/core": "catalog:",
    "@wisemen/eslint-config-vue": "catalog:",
    "eslint": "catalog:",
    "highlight-words": "^2.0.0",
    "histoire": "1.0.0-alpha.2",
    "husky": "catalog:",
    "reka-ui": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  }
}
