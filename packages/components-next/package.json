{
  "name": "@wisemen/vue-core-components",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "version": "2.0.0-beta.0",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "prerelease": "pnpm version prerelease --preid=alpha",
    "build": "pnpm type-check && pnpm build-only && pnpm pack",
    "precommit": "pnpm type-check && test:unit && pnpm build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc -p tsconfig.check.json --noEmit",
    "pub:release": "pnpm publish --access public",
    "test:unit": "vitest --run",
    "test": "vitest --run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "cleanup": "rimraf dist/ node_modules/ .turbo/",
    "story:preview": "histoire preview",
    "prepare": "husky"
  },
  "peerDependencies": {
    "motion-v": ">=1.6.0",
    "reka-ui": ">=2.2.0",
    "temporal-polyfill": ">=0.3.0",
    "vue": ">=3.5.0",
    "vue-i18n": ">=11.0.0"
  },
  "dependencies": {
    "tailwind-variants": "catalog:",
    "vue-sonner": "1.3.0"
  },
  "devDependencies": {
    "@fontsource-variable/inter": "catalog:",
    "@googlemaps/js-api-loader": "1.16.8",
    "@histoire/plugin-vue": "1.0.0-alpha.2",
    "@internationalized/date": "3.5.4",
    "@tailwindcss/vite": "catalog:",
    "@tsconfig/node18": "18.2.4",
    "@types/google.maps": "3.58.1",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/test-utils": "catalog:",
    "@vue/tsconfig": "catalog:",
    "@vueuse/core": "catalog:",
    "@vueuse/router": "catalog:",
    "@wisemen/eslint-config-vue": "catalog:",
    "blurhash": "2.0.5",
    "eslint": "catalog:",
    "globals": "^16.3.0",
    "histoire": "1.0.0-alpha.2",
    "husky": "catalog:",
    "jsdom": "catalog:",
    "libphonenumber-js": "1.11.3",
    "maska": "3.0.0",
    "sass": "1.77.5",
    "tailwind-merge": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "vite": "6.3.5",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:",
    "vue-tsc": "catalog:"
  }
}
