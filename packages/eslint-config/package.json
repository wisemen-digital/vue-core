{
  "name": "@wisemen/eslint-config-vue",
  "type": "module",
  "version": "1.7.3",
  "description": "Eslint config for Vue.js projects",
  "author": "Wisemen",
  "license": "ISC",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "pub:beta": "pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm publish --access public",
    "eslint": "eslint . --fix",
    "cleanup": "rimraf dist/ node_modules/ .turbo/",
    "build": " tsdown --clean --dts",
    "lint": "eslint . && oxlint . --type-aware",
    "eslint-stats": "eslint . --stats -f json",
    "test": "ts-node createFiles.ts",
    "pack": "pnpm pack"
  },
  "peerDependencies": {
    "eslint": "^9.31.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "5.3.0",
    "@eslint/eslintrc": "3.3.1",
    "@intlify/eslint-plugin-vue-i18n": "4.1.0",
    "@typescript-eslint/parser": "8.43.0",
    "@vitest/eslint-plugin": "1.3.9",
    "@wisemen/eslint-plugin-vue": "^0.0.1",
    "eslint": "9.35.0",
    "eslint-flat-config-utils": "^2.1.1",
    "eslint-plugin-better-tailwindcss": "^3.7.8",
    "eslint-plugin-check-file": "3.3.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-newline-destructuring": "^1.2.2",
    "eslint-plugin-oxlint": "^1.15.0",
    "eslint-plugin-path": "2.0.3",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-project-structure": "^3.13.3",
    "eslint-plugin-require-explicit-generics": "1.0.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.2.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-typegen": "^2.3.0",
    "globals": "^16.4.0",
    "oxlint": "^1.15.0",
    "oxlint-tsgolint": "^0.2.0"
  },
  "devDependencies": {
    "tailwindcss": "^4.1.13",
    "ts-node": "10.9.2",
    "tsdown": "^0.15.1",
    "tsx": "^4.20.5",
    "vitest": "^3.2.4"
  }
}
