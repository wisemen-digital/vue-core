{
  "name": "@vue-core/mcp",
  "type": "module",
  "version": "0.1.0",
  "description": "MCP server for local Vue Core packages",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "vue-core-mcp": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "cleanup": "rimraf dist/ node_modules/ .turbo/",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node dist/index.mjs",
    "test": "vitest --run",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.20.0",
    "simple-git": "3.28.0"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@wisemen/eslint-config-vue": "catalog:",
    "eslint": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  }
}
