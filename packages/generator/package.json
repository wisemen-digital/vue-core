{
  "name": "@wisemen/vue-core-generator",
  "type": "module",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "vue-gen": "./dist/index.mjs"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "unbuild && pnpm pack",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chalk": "5.4.1",
    "change-case": "5.4.4",
    "commander": "13.1.0",
    "ejs": "3.1.10",
    "fs-extra": "11.3.0",
    "inquirer": "12.5.0"
  },
  "devDependencies": {
    "@types/ejs": "3.1.5",
    "@types/fs-extra": "11.0.4",
    "@types/inquirer": "9.0.7",
    "@types/node": "22.13.14",
    "@wisemen/eslint-config-vue": "1.0.0-next.1",
    "bumpp": "9.8.1",
    "eslint": "9.14.0",
    "esno": "4.8.0",
    "pnpm": "9.12.3",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "unbuild": "2.0.0",
    "vite": "5.4.10",
    "vitest": "2.1.4"
  }
}
