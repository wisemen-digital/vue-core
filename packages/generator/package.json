{
  "name": "@wisemen/vue-core-generator",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "version": "1.2.0",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typings": "./dist/index.d.mts",
  "bin": {
    "vue-gen": "./dist/index.mjs"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint .",
    "dev": "tsdown --watch",
    "type-check": "tsc --noEmit",
    "cleanup": "rimraf dist/ node_modules/ .turbo/",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chalk": "catalog:build",
    "change-case": "catalog:generator",
    "commander": "catalog:build",
    "ejs": "catalog:generator",
    "fs-extra": "catalog:generator",
    "zod": "catalog:general"
  },
  "devDependencies": {
    "@types/ejs": "catalog:types",
    "@types/node": "catalog:types",
    "@wisemen/eslint-config-vue": "workspace:*",
    "bumpp": "catalog:build",
    "eslint": "catalog:lint",
    "rimraf": "catalog:build",
    "tsdown": "catalog:build",
    "typescript": "catalog:build",
    "vitest": "catalog:test"
  }
}
