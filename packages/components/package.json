{
  "name": "@wisemen/vue-core",
  "type": "module",
  "version": "0.1.17",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "web-types": "./dist/web-types.json",
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "pnpm type-check && pnpm build-only && npm pack && cpx web-types.json dist/web-types.json",
    "precommit": "pnpm type-check && test:unit && pnpm build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "pub:release": "pnpm publish --access public",
    "gen:web-types": "vue-docgen-web-types",
    "test:unit": "vitest --run",
    "test": "vitest --run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "vue": "3.4.35",
    "vue-i18n": "9.13.1"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "1.16.8",
    "@internationalized/date": "3.5.4",
    "@vuepic/vue-datepicker": "9.0.2",
    "axios": "1.7.3",
    "date-fns": "3.6.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "18.2.4",
    "@types/dom-view-transitions": "1.0.5",
    "@types/google.maps": "3.58.1",
    "@types/node": "22.9.0",
    "@vitejs/plugin-vue": "5.1.4",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.5.1",
    "@vueuse/core": "11.2.0",
    "@vueuse/router": "11.2.0",
    "@wisemen/eslint-config-vue": "0.2.0",
    "autoprefixer": "10.4.20",
    "clsx": "2.1.1",
    "cpx": "1.5.0",
    "eslint": "9.14.0",
    "i18n-iso-countries": "7.12.0",
    "jsdom": "25.0.1",
    "libphonenumber-js": "1.11.13",
    "maska": "3.0.3",
    "minimist": "1.2.8",
    "radix-vue": "1.9.8",
    "sass": "1.80.6",
    "tailwind-merge": "2.5.4",
    "tailwind-variants": "0.2.1",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vite-plugin-dts": "3.9.1",
    "vitest": "2.1.4",
    "vue-component-type-helpers": "2.1.10",
    "vue-docgen-web-types": "0.1.8",
    "vue-router": "4.4.5",
    "vue-tsc": "2.1.10",
    "zod": "3.23.8"
  }
}
