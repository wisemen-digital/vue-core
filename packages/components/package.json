{
  "name": "@wisemen/vue-core",
  "type": "module",
  "version": "1.0.0-alpha.17",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "web-types": "./dist/web-types.json",
  "files": [
    "./dist"
  ],
  "scripts": {
    "prerelease": "pnpm version prerelease --preid=alpha",
    "dev": "vite --config vite.playground.config.ts dev",
    "build": "pnpm type-check && pnpm build-only && npm pack && cpx web-types.json dist/web-types.json",
    "precommit": "pnpm type-check && test:unit && pnpm build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc -p tsconfig.check.json --noEmit",
    "pub:release": "pnpm publish --access public",
    "gen:web-types": "vue-docgen-web-types",
    "test:unit": "vitest --run",
    "test": "vitest --run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "vue": "3.5.12",
    "vue-i18n": "10.0.4"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "1.16.8",
    "@internationalized/date": "3.7.0",
    "@tailwindcss/vite": "4.0.3",
    "axios": "1.7.3",
    "date-fns": "4.1.0",
    "defu": "6.1.4",
    "motion-v": "0.8.1",
    "postcss": "8.5.1",
    "reka-ui": "1.0.0-alpha.9",
    "tailwindcss": "4.0.3"
  },
  "devDependencies": {
    "@fontsource-variable/inter": "^5.1.1",
    "@tsconfig/node18": "18.2.4",
    "@types/dom-view-transitions": "1.0.5",
    "@types/google.maps": "3.58.1",
    "@types/node": "22.13.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "@vueuse/core": "12.5.0",
    "@vueuse/router": "12.5.0",
    "@wisemen/eslint-config-vue": "1.0.0-next.0",
    "autoprefixer": "10.4.20",
    "clsx": "2.1.1",
    "cpx": "1.5.0",
    "eslint": "9.19.0",
    "i18n-iso-countries": "7.13.0",
    "jsdom": "26.0.0",
    "libphonenumber-js": "1.11.19",
    "maska": "3.0.4",
    "minimist": "1.2.8",
    "sass": "1.83.4",
    "tailwind-merge": "3.0.1",
    "tailwind-variants": "0.3.1",
    "typescript": "5.7.3",
    "vite": "6.0.11",
    "vite-plugin-dts": "4.5.0",
    "vitest": "3.0.4",
    "vue-router": "4.5.0",
    "vue-tsc": "2.2.0",
    "zod": "3.24.1"
  }
}
