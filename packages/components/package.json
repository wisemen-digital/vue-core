{
  "name": "@wisemen/vue-core",
  "type": "module",
  "version": "0.0.64",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "web-types": "./dist/web-types.json",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "pnpm type-check && pnpm test:unit && pnpm build-only && npm pack && cp web-types.json dist/web-types.json",
    "build-only": "vite build",
    "type-check": "vue-tsc -p tsconfig.check.json --noEmit",
    "pub:release": "pnpm publish --access public",
    "gen:web-types": "vue-docgen-web-types",
    "test:unit": "vitest --run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/dom-view-transitions": "^1.0.4",
    "@types/node": "^18.19.31",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.9.0",
    "@vueuse/router": "^10.9.0",
    "@wisemen/eslint-config-vue": "0.0.15",
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.1",
    "cva": "1.0.0-beta.1",
    "eslint": "8.57",
    "jsdom": "^24.0.0",
    "libphonenumber-js": "^1.10.61",
    "maska": "^2.1.11",
    "minimist": "^1.2.8",
    "radix-vue": "^1.7.3",
    "sass": "^1.75.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-dts": "^3.9.0",
    "vitest": "^1.5.3",
    "vue": "^3.4.26",
    "vue-docgen-web-types": "^0.1.8",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2",
    "vue-tsc": "^2.0.15",
    "zod": "^3.23.5"
  }
}
