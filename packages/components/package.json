{
  "name": "@wisemen/vue-core",
  "type": "module",
  "version": "1.0.0-alpha.42",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "web-types": "./dist/web-types.json",
  "files": [
    "./dist"
  ],
  "scripts": {
    "prerelease": "pnpm version prerelease --preid=alpha",
    "dev": "vite --config vite.playground.config.ts dev",
    "build-deprecated": "pnpm type-check && pnpm build-only && npm pack && cpx web-types.json dist/web-types.json",
    "precommit": "pnpm type-check && test:unit && pnpm build-only",
    "build-only": "vite build",
    "cleanup": "rimraf dist/ node_modules/ .turbo/",
    "type-check": "vue-tsc -p tsconfig.check.json --noEmit",
    "pub:release": "pnpm publish --access public",
    "gen:web-types": "vue-docgen-web-types",
    "test:unit": "vitest --run",
    "test": "vitest --run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "peerDependencies": {
    "vue": "3.5.12",
    "vue-i18n": "10.0.4"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "1.16.8",
    "@internationalized/date": "3.5.4",
    "@tailwindcss/vite": "catalog:",
    "@vuepic/vue-datepicker": "9.0.2",
    "axios": "1.8.4",
    "conventional-changelog": "6.0.0",
    "date-fns": "3.6.0",
    "defu": "6.1.4",
    "motion-v": "catalog:",
    "postcss": "8.4.38",
    "reka-ui": "2.1.0",
    "tailwind-variants": "catalog:",
    "tailwindcss": "catalog:"
  },
  "devDependencies": {
    "@fontsource-variable/inter": "catalog:",
    "@tsconfig/node18": "18.2.4",
    "@types/dom-view-transitions": "1.0.4",
    "@types/google.maps": "3.55.4",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/test-utils": "catalog:",
    "@vue/tsconfig": "catalog:",
    "@vueuse/core": "catalog:",
    "@vueuse/router": "catalog:",
    "@wisemen/eslint-config-vue": "catalog:",
    "autoprefixer": "10.4.19",
    "clsx": "2.1.1",
    "cpx": "1.5.0",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "libphonenumber-js": "1.11.3",
    "maska": "3.0.0",
    "minimist": "1.2.8",
    "sass": "1.77.5",
    "tailwind-merge": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:",
    "vue-router": "catalog:",
    "vue-tsc": "catalog:",
    "zod": "catalog:"
  }
}
