{
  "name": "@wisemen/vue-core-auth",
  "type": "module",
  "version": "0.0.4",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "1.7.7",
    "pkce-challenge": "4.1.0"
  },
  "devDependencies": {
    "@types/node": "22.9.0",
    "@wisemen/eslint-config-vue": "0.2.0",
    "axios-mock-adapter": "2.1.0",
    "bumpp": "9.8.1",
    "eslint": "9.14.0",
    "esno": "4.8.0",
    "pnpm": "9.12.3",
    "rimraf": "6.0.1",
    "typescript": "5.6.3",
    "unbuild": "2.0.0",
    "vite": "5.4.10",
    "vitest": "2.1.4",
    "vitest-localstorage-mock": "0.1.2"
  }
}
