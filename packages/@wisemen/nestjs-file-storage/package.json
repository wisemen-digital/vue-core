    "prerelease": "npm run build",
    "release": "pnpx release-it"
  },
  "devDependencies": {
    "@wisemen/eslint-config-nestjs": "catalog:lint",
    "expect": "catalog:test"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "catalog:backend",
    "@aws-sdk/lib-storage": "catalog:backend",
    "@aws-sdk/s3-request-presigner": "catalog:backend",
    "@azure/storage-blob": "catalog:backend",
    "@nestjs/common": "catalog:backend",
    "class-transformer": "catalog:backend",
    "class-validator": "catalog:backend",
    "dayjs": "catalog:backend"
  },
