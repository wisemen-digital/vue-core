{
  "name": "@wisemen/nestjs-file-storage",
  "version": "0.0.9",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc",
    "lint": "eslint --cache",
