{
  "name": "@wisemen/eslint-plugin-vue",
  "type": "module",
  "version": "0.0.1",
  "description": "Eslint rules extended for Wisemen",
  "license": "MIT",
  "homepage": "https://github.com/wisemen-digital/vue-core.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/wisemen-digital/vue-core.git",
    "directory": "."
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typings": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "pub:release": "pnpm publish --access public",
    "cleanup": "rimraf dist/ node_modules/ .turbo/",
    "lint": "pnpm eslint --fix .",
    "test": "vitest --run",
    "typecheck": "pnpm tsc"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "catalog:peer",
    "eslint": "catalog:peer",
    "typescript": "catalog:peer"
  },
  "dependencies": {
    "@typescript-eslint/scope-manager": "catalog:lint",
    "@typescript-eslint/type-utils": "catalog:lint",
    "@typescript-eslint/utils": "catalog:lint",
    "graphemer": "catalog:general",
    "ts-api-utils": "catalog:general"
  },
  "devDependencies": {
    "@wisemen/eslint-config-vue": "workspace:*",
    "eslint": "catalog:lint",
    "rimraf": "catalog:build",
    "tsdown": "catalog:build",
    "typescript": "catalog:build"
  }
}
