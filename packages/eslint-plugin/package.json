{
  "name": "@wisemen/eslint-plugin-vue",
  "type": "module",
  "version": "0.0.1",
  "description": "Eslint rules extended for Wisemen",
  "license": "MIT",
  "homepage": "https://github.com/wisemen-digital/vue-core.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/wisemen-digital/vue-core.git",
    "directory": "."
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "typings": "./dist/index.d.ts",

  "scripts": {
    "build": "tsdown",
    "pub:release": "pnpm publish --access public",
    "clean": "rimraf dist/ coverage/",
    "lint": "pnpm eslint --fix .",
    "test": "pnpm test",
    "typecheck": "pnpm tsc"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^8.57.0 || ^9.0.0",
    "typescript": ">=4.8.4 <5.9.0"
  },
  "dependencies": {
    "@eslint-community/regexpp": "^4.10.0",
    "@typescript-eslint/scope-manager": "8.35.0",
    "@typescript-eslint/type-utils": "8.35.0",
    "@typescript-eslint/utils": "8.35.0",
    "@typescript-eslint/visitor-keys": "8.35.0",
    "graphemer": "^1.4.0",
    "ignore": "^7.0.0",
    "natural-compare": "^1.4.0",
    "ts-api-utils": "^2.1.0"
  },
  "devDependencies": {
    "@types/mdast": "^4.0.3",
    "@types/natural-compare": "*",
    "@typescript-eslint/rule-tester": "8.35.0",
    "@vitest/coverage-v8": "^3.1.3",
    "ajv": "^6.12.6",
    "cross-fetch": "*",
    "eslint": "*",
    "json-schema": "*",
    "markdown-table": "^3.0.3",
    "marked": "^15.0.0",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-mdx": "^3.0.0",
    "micromark-extension-mdxjs": "^3.0.0",
    "rimraf": "*",
    "title-case": "^4.0.0",
    "tsdown": "^0.12.9",
    "tsx": "*",
    "typescript": "*",
    "unist-util-visit": "^5.0.0",
    "vitest": "^3.1.3"
  }
}
