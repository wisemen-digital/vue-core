{
  "name": "@wisemen/vue-core-query",
  "type": "module",
  "version": "0.0.34",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "web-types": "./dist/web-types.json",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "pnpm type-check && pnpm build-only && npm pack && pnpm gen:web-types",
    "precommit": "pnpm type-check && pnpm test && pnpm build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "pub:release": "pnpm publish --access public",
    "gen:web-types": "vue-docgen-web-types",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --run",
    "dev": "vite build --watch"
  },
  "dependencies": {
    "@tanstack/vue-query": "catalog:"
  },
  "devDependencies": {
    "@tsconfig/node18": "18.2.4",
    "@types/dom-view-transitions": "1.0.6",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/language-server": "2.2.8",
    "@vue/test-utils": "catalog:",
    "@vue/tsconfig": "catalog:",
    "@wisemen/eslint-config-vue": "catalog:",
    "@wisemen/vue-core": "1.0.0-alpha.41",
    "@wisemen/vue-core-components": "1.7.0",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "minimist": "1.2.8",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-component-type-helpers": "2.2.8",
    "vue-docgen-web-types": "0.1.8",
    "vue-tsc": "catalog:"
  }
}
