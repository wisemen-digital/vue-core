{
  "name": "@wisemen/open-api-plugins",
  "type": "module",
  "version": "0.0.3-beta.0",
  "description": "OpenAPI plugins for code generation",
  "license": "MIT",
  "homepage": "https://github.com/wisemen-digital/vue-core.git",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wisemen-digital/vue-core.git",
    "directory": "."
  },
  "keywords": [
    "openapi",
    "codegen",
    "plugins"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsdown",
    "pub:release": "pnpm publish --access public",
    "pub:beta": "pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm publish --no-git-checks --access public --tag next",
    "cleanup": "rimraf dist/ node_modules/ .turbo/",
    "lint": "pnpm eslint --fix .",
    "test": "vitest --run",
    "typecheck": "pnpm tsc"
  },
  "peerDependencies": {
    "@hey-api/openapi-ts": "^0.81.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@wisemen/eslint-config-vue": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  }
}
