{
  "name": "@wisemen/open-api-plugins",
  "type": "module",
  "version": "1.0.1",
  "description": "OpenAPI plugins for code generation",
  "license": "MIT",
  "homepage": "https://github.com/wisemen-digital/vue-core.git",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wisemen-digital/vue-core.git",
    "directory": "."
  },
  "keywords": [
    "openapi",
    "codegen",
    "plugins"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typings": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "pub:release": "pnpm publish --access public",
    "pub:beta": "pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm publish --no-git-checks --access public --tag next",
    "cleanup": "rimraf dist/ node_modules/ .turbo/",
    "lint": "eslint .",
    "test": "vitest --run",
    "typecheck": "pnpm tsc"
  },
  "peerDependencies": {
    "@hey-api/openapi-ts": "catalog:peer"
  },
  "devDependencies": {
    "@wisemen/eslint-config-vue": "workspace:*",
    "tsdown": "catalog:build",
    "typescript": "catalog:build"
  }
}
