{
  "name": "@wisemen/open-api-plugins",
  "type": "module",
  "version": "0.0.2",
  "description": "OpenAPI plugins for code generation",
  "license": "MIT",
  "homepage": "https://github.com/wisemen-digital/vue-core.git",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wisemen-digital/vue-core.git",
    "directory": "."
  },
  "keywords": [
    "openapi",
    "codegen",
    "plugins"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsdown",
    "pub:release": "pnpm publish --access public",
    "cleanup": "rimraf dist/ node_modules/ .turbo/",
    "lint": "pnpm eslint --fix .",
    "test": "pnpm test",
    "typecheck": "pnpm tsc"
  },
  "peerDependencies": {
    "@hey-api/openapi-ts": "^0.81.0"
  },
  "dependencies": {
    "json-schema-faker": "0.5.8"
  },
  "devDependencies": {
    "tsdown": "0.14.2"
  }
}
